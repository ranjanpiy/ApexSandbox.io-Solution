public Integer maxConsecutiveOnes(Integer[] numbers) {
    Integer count=0,maxCount =0;
    for (Integer i=0;i<numbers.size();i++)
    {
        if(numbers[i] ==1 )
        count++;
        else if(numbers[i] ==0 )
        {maxCount=count;
        count=0;}
        else if( i== (numbers.size()-1))
        maxCount=count;
    }
    return maxCount;
}