public Boolean duplicateContacts(Contact c1, Contact c2) {
    Boolean result;
    if(c1.Email == null || c1.Phone == null || c2.Email == null || c2.Phone == null )
    result = false;
    else if (c1.Email != null  && c2.Phone != null && ((c1.Email == c2.Email))|| (c1.Phone == c2.Phone))
    result= true;

    return result;

}