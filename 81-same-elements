    Boolean result;
    Set<Integer> setNums1 = new Set<Integer>(nums1);
    Set<Integer> setNums2 = new Set<Integer>(nums2);
   /* if(setNums1.isEmpty() == true && setNums2.isEmpty() == true)
    result = true;
    else if(setNums2.equals(setNums1))
    result = true;
    else
    result=false;*/
    result = setNums2.equals(setNums1);
    return result;
}