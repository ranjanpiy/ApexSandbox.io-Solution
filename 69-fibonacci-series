public List<Integer> fibonacciSeries(Integer n) {
    List<Integer> numberList = new List<Integer>();
    Integer n1=0,n2=1,fN;
    if( n ==1 )
    numberList = {1};
    else if(n ==2)
     numberList = {1,1};
    {
        for( Integer i=2;i<=n;i++)
        {
            fN = n1+n2;
            n1=n2;
            n2=fN;
            numberList.add(fN);
        }
    }
    return numberList;
}