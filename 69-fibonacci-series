public List<Integer> fibonacciSeries(Integer n) {
    List<Integer> numberList = new List<Integer>();
    Integer sum=0;
    if( n ==1 )
    { sum =1;
    numberList.add(sum);}
    else
    {
        for( Integer i=0;i<n;i++)
        {
            sum =i+sum;
            numberList.add(sum);
        }
    }
    return numberList;
}